<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AlphaMarketPDV</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-grid.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/all.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.0/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.0/css/bootstrap-grid.min.css"
              asp-fallback-href="~/lib/booststrap/dist/css/bootstrap-grid.min.css"
              asp-fallback-test-class="sr-only"
              asp-fallback-test-property="position"
              asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.css"
              asp-fallback-href="~/lib/font-awesome/css/all.css"
              asp-fallback-test-class="sr-only"
              asp-fallback-test-property="position"
              asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
    @RenderSection("Styles", required: false)
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                <img class="d-inline-block align-text-top" src="~/images/app/Logo_Transp.png" width="30" height="30" alt="Logo da aplicação">&nbsp; Alpha Market PDV
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarToggler" aria-controls="navbarToggler" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarToggler">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item active">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Cadastro
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Produtos" asp-action="Index">Produtos</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Fornecedores</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="FormasPagamento" asp-action="Index">Formas de Pagamento</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="UnidadesMedida" asp-action="Index">Unidades de Medida</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Categorias" asp-action="Index">Categorias de Produto</a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Estoque</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="DropdownMenuVenda" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Venda
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="DropdownMenuVenda">
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Acompanhar Vendas</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Processar Venda</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Consultar Preço</a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="DropdownMenuCaixa" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Caixa
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="DropdownMenuCaixa">
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Fluxo de Caixa</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Operar</a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="DropdownMenuManutencao" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Manutenção
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="DropdownMenuManutencao">
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Usuarios" asp-action="Index">Usuários</a>
                            </li>
                            <li>
                                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Alteração de Senha</a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">Sobre</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">Contato</a>
                    </li>
                </ul>
                <form class="d-flex">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success" type="submit">Search</button>
                </form>
            </div>
        </div>
    </nav>

    <partial name="_CookieConsentPartial" />

    <div class="container" style="margin-top: 20px;">
        <div class="row">
            <div class="col-1">
                <div class="row">

                </div>
            </div>
            <div class="col-10">
                @RenderBody()

                <hr />
                <footer>
                    <p>&copy; 2021 - Alpha Market PDV - Versão 1.0 / Desenvolvidor por <a href="https://www.linkedin.com/in/rafael-de-oliveira-silva-531767a1/">Rafael Silva</a></p>
                </footer>
            </div>
            <div class="col-1">
                <div class="row">

                </div>
            </div>
        </div>
    </div>

    <environment include="Development">
        <script src="~/lib/popper.js/umd/popper.js"></script>
        <script src="~/lib/jquery/jquery.slim.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    </environment>

    <environment exclude="Development">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.9.3/umd/popper.min.js"
                asp-fallback-src="~/lib/popper.js/umd/popper.js">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
                asp-fallback-src="~/lib/jquery/jquery.slim.min.js">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.0/js/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js">
        </script>
    </environment>

    @RenderSection("ScriptPage", required: false)
</body>
</html>
